{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-len": [
      "error", 100
    ],
    "indent": [
      "error",
      2,
      { "SwitchCase": 1 }
    ],
    "object-curly-spacing": [
      "error", "always"
    ],
    "space-infix-ops": "error"
  }
}
